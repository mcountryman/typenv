{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-submodule-imports": false,
    "max-line-length": [
      true,
      {
        "limit": 90,
        "check-strings": true,
        "check-regex": true
      }
    ]
  }
}